{
  "name": "undernet",
  "version": "4.1.0",
  "homepage": "https://undernet.io",
  "description": "A modular, configuration-first front-end framework. No strings.",
  "author": "George Treviranus",
  "license": "MIT",
  "private": false,
  "main": "js/dist/index.js",
  "module": "dist/undernet.bundle.esm.js",
  "browser": "dist/undernet.bundle.min.js",
  "sass": "scss/undernet.scss",
  "style": "dist/undernet.css",
  "scripts": {
    "watch": "npm run js:build && webpack-dev-server --config config/webpack.dev.js --progress --open",
    "build": "webpack --config config/webpack.prod.js --progress",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js -c ./.eslintrc.js --ignore-path .eslintignore",
    "build:dist": "npm run build:lint && npm run js:build && npm run js:rollup && npm run css:build && npm run css:minify",
    "build:dist:release": "npm run clean && npm run build:lint && npm run scss:prepublish && npm run css:prepublish && npm run js:prepublish && npm run create-hashes",
    "build:lint": "npm run js:lint && npm run scss:lint",
    "clean": "rm -rf dist/ && mkdir dist/",
    "js:build": "BABEL_ENV=dist babel js/src/ -d js/dist/ --no-comments -s",
    "js:watch": "BABEL_ENV=dist babel js/src/ --watch -d js/dist/ --no-comments -s",
    "js:lint": "prettier --write 'js/src/**/*.js'",
    "js:rollup": "BABEL_ENV=dist rollup -c config/rollup.config.js",
    "js:modules:zip": "cd js/dist/ && zip -r ../../dist/undernet.modules.js.zip .",
    "js:zip": "cd dist/ && zip -r undernet.js.zip . -x *.css *.zip *.css.map",
    "js:prepublish": "npm run js:build && npm run js:rollup && npm run js:modules:zip && npm run js:zip",
    "css:minify": "node-sass --source-map true --output-style compressed scss/undernet.scss dist/undernet.min.css",
    "css:build": "node-sass --source-map true scss/undernet.scss dist/undernet.css",
    "css:package": "npm run css:build && npm run css:minify",
    "css:prepublish": "npm run css:package && cd dist/ && zip -r undernet.css.zip . -x *.js *.zip *.js.map",
    "scss:lint": "prettier --write 'scss/**/*.scss'",
    "scss:prepublish": "cd scss/ && zip -r ../dist/undernet.scss.zip .",
    "update-version": "node scripts/update-version.js",
    "create-hashes": "node scripts/create-hashes.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.9",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-webpack-aliases": "^1.1.3",
    "canvas": "^2.3.1",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^1.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jshashes": "^1.0.7",
    "lodash": "^4.17.11",
    "lottie-web": "^5.4.4",
    "markdown-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "prismjs": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-feather": "^1.1.6",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.0.6",
    "react-router-dom": "^4.3.1",
    "react-router-last-location": "^2.0.0-beta.1",
    "react-spinners": "^0.5.1",
    "resolve-url-loader": "^3.0.1",
    "rollup": "^1.4.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "yargs": "^13.2.1"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.9.0"
  },
  "keywords": [
    "scss",
    "javascript",
    "js",
    "css",
    "frontend",
    "framework",
    "web"
  ],
  "files": [
    "dist/*.{css,js,map}",
    "js/{src,dist}/*.{js,map}",
    "scss/**/*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geotrev/undernet.git"
  },
  "bugs": {
    "url": "https://github.com/geotrev/undernet/issues"
  },
  "jest": {
    "clearMocks": true,
    "restoreMocks": true,
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/config/__mocks__/emptyMock.js",
      "\\.(jpg|jpeg|png|gif|md|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/config/__mocks__/emptyMock.js",
      "^projectRoot/(.*?)$": "<rootDir>/$1",
      "^routes/(.*?)$": "<rootDir>/site/routes/$1",
      "^components/(.*?)$": "<rootDir>/site/components/$1",
      "^helpers/(.*?)$": "<rootDir>/site/helpers/$1",
      "^pages/(.*?)$": "<rootDir>/site/pages/$1",
      "^assets/(.*?)$": "<rootDir>/site/assets/$1",
      "^docs/(.*?)$": "<rootDir>/site/docs/$1",
      "^undernet/(.*?)$": "<rootDir>/js/src/$1"
    },
    "roots": [
      "<rootDir>/site/",
      "<rootDir>/js/"
    ],
    "setupFiles": [
      "<rootDir>/config/jest.config.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/site/**/*.spec.js",
      "<rootDir>/js/**/*.spec.js"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "verbose": false
  }
}
